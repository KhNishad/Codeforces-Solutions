#include<bits/stdc++.h>
using namespace std;

int main ()
{
    int time  = 240;
    int a , b , i , sum=0, val=0, cnt=0;
    cin>>a>>b;
    for(i=1;i<=a;i++)
    {
        val = 0;
        sum += (i*5);

        val  = sum+b;

        if(val <= time)
        {

            cnt++;

        }
    }
    cout<<cnt<<endl;
}

